<div id="login">
  <%= content_tag :h2, "Please enter the 2FA code recieved by SMS" %>
  <%= active_admin_form_for(:two_factor_authentication, :as => resource_name, :html => {:method => 'put'}) do |f|
    f.inputs do
      f.input :code, input_html: { name: :code }
    end
    f.actions do
      f.action :submit, :label => "Submit"
    end
  end %>

  <%= link_to "Sign out", destroy_user_session_path, :method => :delete %>
</div>
